void setup() {


PImage img;
img=loadImage("orologio.jpg");
image (img,0,0,500,500);
size (500,500);
}


void draw(){
stroke (0);
line (50,150,250,240);
strokeWeight (2);

pushMatrix();
rotate (frameRate *1);
translate (400,500);
popMatrix();

stroke (255,0,0);
line (90,200,250,240);
strokeWeight (2);

pushMatrix();
rotate (frameRate *1);
translate (200,200);
popMatrix();

}

 
       
